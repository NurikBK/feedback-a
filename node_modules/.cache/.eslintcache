Feedback(id, updItem) {\n    setFeedback(feedback.map((item) =>  item.id === id ? {...item, ...updItem} : item))\n  }\n\n  function editFeedback(item) {\n    setFeedbackEdit({\n      item,\n      edit: true,\n    });\n  }\n\n  return (\n    <FeedbackContext.Provider\n      value={{\n        feedback,\n        feedbackEdit,\n        handleDelete,\n        handleAdd,\n        editFeedback,\n        updateFeedback,\n      }}\n    >\n      {children}\n    </FeedbackContext.Provider>\n  );\n};\n\nexport default FeedbackContext;\n",{"ruleId":"75","severity":1,"message":"76","line":16,"column":53,"nodeType":"77","messageId":"78","endLine":16,"endColumn":55},"eqeqeq","Expected '!==' and instead saw '!='.","BinaryExpression","unexpected"]